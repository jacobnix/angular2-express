{
  "name": "angular2-express",
  "version": "1.0.0",
  "description": "Starter application of Angular2 with Express",
  "main": "server/app.js",
  "license": "MIT",
  "scripts": {
    "start": "concurrently --kill-others \"ng serve --aot=true --progress=false --proxy-config proxy.conf.json\" \"npm run _server:run\"",
    "client": "concurrently --kill-others \"ng serve --aot=true --progress=false --proxy-config proxy.conf.json\"",
    "server": "npm run _server:run",
    "lint:client": "ng lint",
    "lint:server": "tslint './server/**/*.ts' -c tslint.json --fix",
    "test:client": "ng test",
    "test:server": "mocha dist/server/test",
    "e2e:client": "ng e2e",
    "build": "ng build --prod --sm=false --aot=true --output-path=dist/client && npm run _server:build",
    "_server:run": "tsc -p ./server && concurrently \"tsc -w -p ./server\" \"nodemon dist/server/bin/www.js\" \"cp server/api/swagger.yaml dist/server/api/swagger.yaml\" ",
    "_server:build": "tsc -p ./server",
    "postinstall": "npm run build"
  },
  "author": "Jacob Nix",
  "repository": {
    "url": "https://github.com/jacobnix/angular2-express"
  },
  "private": true,
  "dependencies": {
    "@angular/cli": "^1.0.0",
    "@angular/common": "^4.0.1",
    "@angular/compiler": "^4.0.1",
    "@angular/compiler-cli": "^4.0.1",
    "@angular/core": "^4.0.1",
    "@angular/forms": "^4.0.1",
    "@angular/http": "^4.0.1",
    "@angular/platform-browser": "^4.0.1",
    "@angular/platform-browser-dynamic": "^4.0.1",
    "@angular/router": "^4.0.1",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^2.0.2",
    "@ngrx/store": "^2.2.1",
    "@ngrx/store-devtools": "^3.2.4",
    "@types/body-parser": "^1.16.3",
    "@types/compression": "^0.0.33",
    "@types/cors": "^2.8.1",
    "@types/express": "^4.0.35",
    "@types/js-yaml": "^3.5.30",
    "@types/jsonwebtoken": "^7.2.0",
    "@types/log4js": "0.0.32",
    "@types/node": "^6.0.73",
    "@types/swaggerize-express": "^4.0.28",
    "@types/uuid": "^2.0.29",
    "body-parser": "~1.17.1",
    "chai": "^3.5.0",
    "compression": "^1.6.2",
    "cookie-parser": "~1.3.5",
    "core-js": "^2.4.1",
    "express": "^4.15.2",
    "express-jwt": "^5.1.0",
    "js-yaml": "^3.8.4",
    "jsonwebtoken": "^7.3.0",
    "log4js": "^1.1.1",
    "ngrx-store-freeze": "^0.1.9",
    "normalize.css": "^5.0.0",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.1.0",
    "swaggerize-express": "^4.0.5",
    "ts-helpers": "^1.1.2",
    "ts-node": "~3.0.2",
    "tslint": "^4.5.1",
    "typedi": "^0.5.2",
    "typescript": "^2.2.2",
    "uuid": "^3.0.1",
    "zone.js": "^0.8.9"
  },
  "devDependencies": {
    "@types/chai": "^3.5.2",
    "@types/chai-http": "0.0.30",
    "@types/mocha": "^2.2.41",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "codelyzer": "~2.1.1",
    "concurrently": "^3.1.0",
    "karma": "~1.5.0",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-remap-istanbul": "^0.2.1",
    "mocha": "^3.4.2",
    "mocha-typescript": "^1.1.4",
    "nodemon": "^1.11.0",
    "protractor": "~5.1.0",
    "webdriver-manager": "10.2.5"
  }
}
